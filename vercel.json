{
  "builds": [
    {
      "src": "proxy-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "proxy-server.js"
    },
    {
      "src": "/models/(.*)",
      "dest": "proxy-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ]
}
